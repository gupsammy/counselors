{
  "version": 1,
  "defaults": {
    "timeout": 540,
    "outputDir": "./agents/counselors",
    "readOnly": "bestEffort",
    "maxContextKb": 50,
    "maxParallel": 4
  },
  "tools": {
    "claude": {
      "binary": "/usr/local/bin/claude",
      "readOnly": { "level": "enforced" },
      "extraFlags": ["--model", "opus"]
    },
    "codex": {
      "binary": "/usr/local/bin/codex",
      "readOnly": { "level": "enforced" },
      "extraFlags": ["-m", "gpt-5.3-codex"]
    }
  }
}
