{
  "name": "counselors",
  "version": "0.1.0",
  "description": "Fan out prompts to multiple AI coding agents in parallel",
  "type": "module",
  "bin": {
    "counselors": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "lint": "biome check src/ tests/",
    "lint:fix": "biome check --write src/ tests/"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.0.0",
    "commander": "^13.0.0",
    "ora": "^8.0.0",
    "p-limit": "^6.0.0",
    "strip-ansi": "^7.1.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  },
  "engines": {
    "node": ">=20"
  },
  "files": [
    "dist",
    "assets"
  ],
  "license": "MIT"
}
